using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ReportesApp
{
    class ReporteMasControlVisual
    {
        Reporte reporte;
        WindowsFormsControlLibrary1.UserControl1 controlVisual;

        public ReporteMasControlVisual(Reporte dbReporte, WindowsFormsControlLibrary1.UserControl1 controlVisual)
        {
            this.Reporte = dbReporte;
            this.ControlVisual = controlVisual;
        }

        public Reporte Reporte
        {
            get { return reporte; }
            set { reporte = value; }
        }
        public WindowsFormsControlLibrary1.UserControl1 ControlVisual
        {
            get { return controlVisual; }
            set { controlVisual = value; }
        }
        
        
    }
}

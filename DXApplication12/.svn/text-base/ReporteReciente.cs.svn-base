using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ReporteApp
{
    public class ReporteReciente
    {
        private String numero;
        private String nombreEquipo;
        private DateTime fecha_hora;
        private String estado;
        private String posibleProblema;
        private String observacion;
        private DateTime fecha_hora_fin;

        public DateTime FechaHoraFin
        {
            get { return fecha_hora_fin; }
            set { fecha_hora_fin = value; }
        }
        

        public String Observacion
        {
            get { return observacion; }
            set { observacion = value; }
        }
        

        public String PosibleProblema
        {
            get { return posibleProblema; }
            set { posibleProblema = value; }
        }
        

        public String Estado
        {
            get { return estado; }
            set { estado = value; }
        }
        

        public DateTime FechaHora
        {
            get { return fecha_hora; }
            set { fecha_hora = value; }
        }
        

        public String NombreEquipo
        {
            get { return nombreEquipo; }
            set { nombreEquipo = value; }
        }
        

        public String Numero
        {
            get { return numero; }
            set { numero = value; }
        }
        
    }
}
